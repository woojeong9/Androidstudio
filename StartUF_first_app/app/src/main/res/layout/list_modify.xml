<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <ImageView
        android:id="@+id/Modifyrect"
        android:layout_width="370dp"
        android:layout_height="500dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/modifyrect" />

    <ImageView
        android:id="@+id/Back"
        android:layout_width="30dp"
        android:layout_height="30dp"
        android:layout_marginTop="32dp"
        android:layout_marginEnd="32dp"
        android:layout_marginRight="32dp"
        app:layout_constraintEnd_toEndOf="@+id/Modifyrect"
        app:layout_constraintTop_toTopOf="@+id/Modifyrect"
        app:srcCompat="@drawable/back" />

    <ImageView
        android:id="@+id/Listimage"
        android:layout_width="130dp"
        android:layout_height="130dp"
        android:layout_marginTop="64dp"
        app:layout_constraintEnd_toEndOf="@+id/Modifyrect"
        app:layout_constraintStart_toStartOf="@+id/Modifyrect"
        app:layout_constraintTop_toTopOf="@+id/Modifyrect"
        app:srcCompat="@drawable/listimage" />

    <ImageView
        android:id="@+id/Modifypen"
        android:layout_width="30dp"
        android:layout_height="30dp"
        app:layout_constraintBottom_toBottomOf="@+id/Listimage"
        app:layout_constraintEnd_toEndOf="@+id/Listimage"
        app:srcCompat="@drawable/modify" />

    <ImageView
        android:id="@+id/Listid"
        android:layout_width="185dp"
        android:layout_height="30dp"
        android:layout_marginStart="16dp"
        android:layout_marginLeft="16dp"
        app:layout_constraintBottom_toBottomOf="@+id/Idtext"
        app:layout_constraintStart_toEndOf="@+id/Idtext"
        app:layout_constraintTop_toTopOf="@+id/Idtext"
        app:srcCompat="@drawable/listid" />

    <ImageView
        android:id="@+id/Idtext"
        android:layout_width="25dp"
        android:layout_height="25dp"
        android:layout_marginStart="68dp"
        android:layout_marginLeft="68dp"
        android:layout_marginTop="32dp"
        app:layout_constraintStart_toStartOf="@+id/Modifyrect"
        app:layout_constraintTop_toBottomOf="@+id/imageView10"
        app:srcCompat="@drawable/idtext" />

    <ImageView
        android:id="@+id/Pwtext"
        android:layout_width="33dp"
        android:layout_height="33dp"
        android:layout_marginStart="68dp"
        android:layout_marginLeft="68dp"
        android:layout_marginTop="16dp"
        app:layout_constraintStart_toStartOf="@+id/Modifyrect"
        app:layout_constraintTop_toBottomOf="@+id/Idtext"
        app:srcCompat="@drawable/pwtext" />

    <ImageView
        android:id="@+id/Listpw"
        android:layout_width="185dp"
        android:layout_height="30dp"
        android:layout_marginStart="10dp"
        android:layout_marginLeft="10dp"
        app:layout_constraintBottom_toBottomOf="@+id/Pwtext"
        app:layout_constraintStart_toEndOf="@+id/Pwtext"
        app:layout_constraintTop_toTopOf="@+id/Pwtext"
        app:layout_constraintVertical_bias="1.0"
        app:srcCompat="@drawable/listid" />

    <ImageView
        android:id="@+id/Complete"
        android:layout_width="70dp"
        android:layout_height="70dp"
        android:layout_marginTop="140dp"
        android:layout_marginBottom="100dp"
        app:layout_constraintBottom_toBottomOf="@+id/Modifyrect"
        app:layout_constraintEnd_toEndOf="@+id/Modifyrect"
        app:layout_constraintStart_toStartOf="@+id/Modifyrect"
        app:layout_constraintTop_toBottomOf="@+id/Listpw"
        app:srcCompat="@drawable/complete" />

    <ImageView
        android:id="@+id/imageView10"
        android:layout_width="170dp"
        android:layout_height="20dp"
        android:layout_marginTop="32dp"
        android:layout_marginBottom="250dp"
        app:layout_constraintBottom_toBottomOf="@+id/Modifyrect"
        app:layout_constraintEnd_toEndOf="@+id/Modifyrect"
        app:layout_constraintStart_toStartOf="@+id/Modifyrect"
        app:layout_constraintTop_toBottomOf="@+id/Listimage"
        app:srcCompat="@drawable/listtitle" />
</androidx.constraintlayout.widget.ConstraintLayout>